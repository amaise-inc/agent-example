# amaise Agent API credentials - obtain from workspace integration settings
LEGALI_AUTH_URL=<>
LEGALI_API_URL=<>
LEGALI_CLIENT_ID=<>
LEGALI_CLIENT_SECRET=<>

# Tenant mapping - map logical department names to tenant UUIDs.
# The lib picks the first LEGALI_TENANTS_* value as the active tenant.
# For single-tenant setups, use TENANT_ID instead.
# TENANT_ID=<>
# LEGALI_TENANTS_DEPARTMENT-1=<>
# LEGALI_TENANTS_DEPARTMENT-2=<>

# Dashboard ID (required for dashboard-processing example)
# DASHBOARD_ID=<>

# Heartbeat poll interval in ms (default: 600000 = 10 min, lower for CI)
# HEARTBEAT_INTERVAL_MS=10000
