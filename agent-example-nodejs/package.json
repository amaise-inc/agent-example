{
  "name": "agent-example-nodejs",
  "version": "1.16.0-alpha",
  "private": true,
  "description": "Minimal amaise Agent REST API example in TypeScript",
  "license": "MIT",
  "type": "module",
  "scripts": {
    "legalcase-lifecycle": "tsx examples/legalcase-lifecycle.ts",
    "sourcefile-processing": "tsx examples/sourcefile-processing.ts",
    "dashboard-processing": "tsx examples/dashboard-processing.ts",
    "validate": "tsx ci/validate.ts",
    "test": "vitest run",
    "typecheck": "tsc --noEmit",
    "lint": "tsc --noEmit && prettier --check .",
    "verify": "npm run lint && npm test",
    "format": "prettier --write .",
    "generate:fetch": "curl -fsSL -o openapi-agent.json https://api.eu.amaise.com/doc/api-documentation/AGENT-SDK",
    "generate": "openapi-ts"
  },
  "dependencies": {
    "dotenv": "^16.4.0",
    "simple-oauth2": "^5.0.0",
    "tsx": "^4.0.0"
  },
  "devDependencies": {
    "@hey-api/openapi-ts": "0.92.3",
    "@types/node": "^22.0.0",
    "@types/simple-oauth2": "^5.0.0",
    "prettier": "^3.0.0",
    "typescript": "^5.7.0",
    "vitest": "^4.0.18"
  },
  "engines": {
    "node": ">=22"
  }
}
